{
  "name": "@vibecaas/design-system-ai-agents",
  "version": "1.0.0",
  "description": "AI-powered agents for design system component generation and management",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "ts-node mcp/server.ts",
    "mcp:start": "ts-node mcp/server.ts",
    "mcp:dev": "nodemon --watch '**/*.ts' --exec 'ts-node' mcp/server.ts",
    "mcp:update-context": "ts-node scripts/update-context.ts",
    "mcp:clear-cache": "ts-node scripts/clear-cache.ts",
    "generate:component": "ts-node cli/generate-component.ts",
    "agent:generate": "ts-node cli/agent-generate.ts",
    "agent:review": "ts-node cli/agent-review.ts",
    "agent:qa": "ts-node cli/agent-qa.ts",
    "agent:docs": "ts-node cli/agent-docs.ts",
    "agent:retrain": "ts-node scripts/retrain-agents.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "logs": "tail -f logs/mcp-server.log"
  },
  "keywords": [
    "design-system",
    "ai",
    "openai",
    "component-generation",
    "automation"
  ],
  "author": "VibeCaaS Team",
  "license": "MIT",
  "dependencies": {
    "openai": "^4.24.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ws": "^8.16.0",
    "node-cache": "^5.1.2",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/ws": "^8.5.10",
    "@types/jest": "^29.5.11",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
